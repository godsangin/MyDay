<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.msproject.myhome.mydays">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3136625326865731~8346285691"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventActivity" />
        <activity
            android:name=".StatisticActivity"
            android:label="@string/title_activity_statistic"
            android:theme="@style/AppTheme" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".SetCategoryActivity" />
        <activity android:name=".InputActivity" />
        <activity android:name=".IntroMainActivity" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".IntroEventActivity"></activity>
    </application>

</manifest>